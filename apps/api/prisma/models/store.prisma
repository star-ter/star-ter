//  store_{city|gu|dong|backarea|commercial}

model StoreCity {
    STDR_YYQU_CD          String //기준_년분기_코드
    MEGA_CD               String //서울시_코드
    MEGA_CD_NM            String //서울시_코드_명
    SVC_INDUTY_CD         String //서비스_업종_코드
    SVC_INDUTY_CD_NM      String //서비스_업종_코드_명
    STOR_CO               Int //점포_수
    SIMILR_INDUTY_STOR_CO Int //유사_업종_점포_수
    OPBIZ_RT              Float //개업_율
    OPBIZ_STOR_CO         Int //개업_점포_수
    CLSBIZ_RT             Float //폐업_률
    CLSBIZ_STOR_CO        Int //폐업_점포_수
    FRC_STOR_CO           Int //프랜차이즈_점포_수

    @@id([STDR_YYQU_CD, MEGA_CD, SVC_INDUTY_CD])
    @@map("store_city")
}

