model user_info {
  id                       String                     @id @default(uuid()) @db.Uuid
  email                    String                     @unique
  password                 String
  nickname                 String
  createdAt                DateTime                   @default(now())
  updatedAt                DateTime                   @updatedAt
  bookmark                 bookmark[]
  real_estate_registration real_estate_registration[]
}

model bookmark {
  id             String    @id
  user_id        String    @db.Uuid
  commercialCode String
  commercialName String
  createdAt      DateTime  @default(now())
  user           user_info @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@unique([user_id, commercialCode])
}
