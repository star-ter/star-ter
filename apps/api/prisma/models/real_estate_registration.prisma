model real_estate_registration {
  id              String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  user_id         String    @db.Uuid
  address         String    @db.VarChar(500)
  phone           String    @db.VarChar(20)
  deposit         BigInt
  monthly_rent    BigInt
  area_pyeong     Decimal   @db.Decimal(6, 2)
  floor_info      String    @db.VarChar(50)
  maintenance_fee BigInt?
  premium         BigInt?
  description     String?
  images          String[]
  created_at      DateTime? @default(now()) @db.Timestamp(6)
  user_info       user_info @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}
